        android:id="@+id/humid_t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1450px"
        android:layout_marginLeft="350px"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text=" Humidity: " />



    <TextView
        android:id="@+id/rpm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1560px"
        android:layout_marginLeft="310px"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:text="@string/icon_sync" />
    <TextView
        android:id="@+id/rpm_t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1545px"
        android:layout_marginLeft="353px"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text=" RPMs: " />



    <TextView
        android:id="@+id/power"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1660px"
        android:layout_marginLeft="310px"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text="@string/icon_power" />
    <TextView
        android:id="@+id/power_t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1650px"
        android:layout_marginLeft="353px"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text=" Power Use: " />



    <TextView
        android:id="@+id/time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1750px"
        android:layout_marginLeft="310px"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text="@string/icon_time" />
    <TextView
        android:id="@+id/time_t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1740px"
        android:layout_marginLeft="353px"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text=" Time: " />
</RelativeLayout>