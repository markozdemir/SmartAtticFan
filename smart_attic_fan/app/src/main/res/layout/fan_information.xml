<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/better_city">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="match_parent"
        android:layout_height="371dp"
        android:layout_marginTop="900px"
        app:srcCompat="@drawable/house" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1150px"
        android:layout_marginLeft="375px"
        android:textColor="@color/black"
        android:textSize="55sp"
        android:text="Attic" />

    <TextView
        android:id="@+id/temp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1360px"
        android:layout_marginLeft="310px"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text="@string/icon_temp" />
    <TextView
        android:id="@+id/temp_t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1350px"
        android:layout_marginLeft="350px"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text=" Temperature: " />



    <TextView
        android:id="@+id/humid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1460px"
        android:layout_marginLeft="310px"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:text="@string/icon_water" />
